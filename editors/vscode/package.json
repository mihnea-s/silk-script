{
    "name": "silk-code",
    "publisher": "SilkScript",
    "displayName": "Silk Code",
    "description": "SilkScript language support",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/mihnea-s/silk-script"
    },
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "silkscript",
                "aliases": [
                    "SilkScript",
                    "silkscript"
                ],
                "extensions": [
                    ".silk"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "silkscript",
                "scopeName": "source.silk",
                "path": "./language-syntax.yaml"
            }
        ]
    }
}